{
  "name": "@x-qdo/connected-react-router",
  "version": "7.0.4",
  "description": "A Redux binding for React Router v6",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "./index.d.ts",
  "sideEffects": false,
  "author": "Supasate Choochaisri",
  "license": "MIT",
  "files": [
    "*.md",
    "*.js",
    "*.ts",
    "esm",
    "lib",
    "umd"
  ],
  "scripts": {
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir esm",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development webpack ./src/index.js --output-path umd/ConnectedReactRouter.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack --mode production ./src/index.js --output-path umd/ConnectedReactRouter.min.js",
    "build": "npm run build:esm & npm run build:commonjs & npm run build:umd & npm run build:umd:min",
    "lint": "eslint .",
    "test": "jest --config ./.jestrc.json",
    "test:watch": "npm run test -- --watch",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "lodash.isequalwith": "^4.4.0",
    "prop-types": "^15.8.1",
    "react-router-dom": "^6.3.0"
  },
  "peerDependencies": {
    "history": "^4.10.1",
    "react": "^18.0.0",
    "react-redux": "^8.0.0",
    "react-router": "^6.0.0",
    "redux": "^4.0.0"
  },
  "optionalDependencies": {
    "immutable": "^4.0.0",
    "seamless-immutable": "^7.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@types/history": "^5.0.0",
    "@types/react": "18.0.10",
    "@types/react-redux": "^7.1.24",
    "@types/react-router": "^5.1.18",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-rewire": "^1.2.0",
    "babel-template": "^6.2.0",
    "babel-types": "^6.2.0",
    "cross-env": "^7.0.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.16.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.0",
    "immutable": "^4.0.0",
    "jest": "^28.1.0",
    "raf": "^3.4.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-test-renderer": "^18.1.0",
    "redux": "^4.2.0",
    "redux-devtools": "^3.7.0",
    "redux-immutable": "^4.0.0",
    "redux-mock-store": "^1.2.1",
    "redux-seamless-immutable": "^0.4.0",
    "rewire": "^6.0.0",
    "seamless-immutable": "^7.1.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/x-qdo/connected-react-router.git"
  },
  "bugs": {
    "url": "https://github.com/x-qdo/connected-react-router/issues"
  }
}
